{
    "name": "ducksms/ducksms-php",
    "version": "1.0.0",
    "description": "Ducksms is a leading SMS messaging service provider offering one-way SMS communication services from your internet enabled computer. The API allow you to directly communicate and manage a SMS with our SMS gateway system.",
    "keywords": [
        "ducksms",
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://ducksms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ducksms",
            "homepage": "https://ducksms.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Ducksms\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Ducksms\\" : "test/" }
    }
}
